{
  "compilerOptions": {
    "target": "es2021",
    "lib": [
      "es2021"
    ],
    "jsx": "react-jsx",
    "module": "es2022",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "types": [
      "./worker-configuration.d.ts",
      "node"
    ],
    "esModuleInterop": true
  },
  "include": [
    "worker-configuration.d.ts",
    "src/**/*.ts"
  ],
  "exclude": [
    "test"
  ]
}